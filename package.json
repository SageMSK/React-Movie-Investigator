{
  "name": "movie-tracking-app",
  "version": "1.0.0",
  "engines": {
    "node": ">= 7.6.0"
  },
  "description": "This app track all of your watched movies",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon ./server.js",
    "assets": "webpack-dev-server --open",
    "dev": "cross-env npm run server npm run assets --names Server",
    "build": "cross-env NODE_ENV=production webpack -p"
  },
  "author": "Min Soo Kim (http://github.com/sagemsk)",
  "license": "ISC",
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-router",
    "boilerplate",
    "starter",
    "webpack-dev-server",
    "webpack"
  ],
  "babel": {
    "presets": [
      "react",
      "stage-1",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cross-env": "^5.0.5",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.16.2",
    "helmet": "^3.8.2",
    "html-to-text": "^3.3.0",
    "joi": "^11.3.4",
    "jsonwebtoken": "^8.1.0",
    "juice": "^4.2.0",
    "mongoose": "^4.12.1",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "^1.9.0",
    "nodemailer": "^4.1.3",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "slug": "^0.9.1",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
